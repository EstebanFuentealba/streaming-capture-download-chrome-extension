{
  "author": "Esteban Fuentealba",
  "browser_action": {
    "default_icon": "icon-19x19.png",
    "default_title": "Capturar!"
  },
  "background": {
    "persistent": true,
    "scripts": [ "background.js" ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2,
  "name": "Streaming download",
  "permissions": [ "tabs", "webRequest","unlimitedStorage", "webRequestBlocking","downloads", "*://*/*" ],
  "short_name": "Streaming download",
  "version": "0.0.0.1"
}
